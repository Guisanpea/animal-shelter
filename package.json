{
  "name": "nest-typescript-starter",
  "private": true,
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "nest": "nest",
    "next": "cd src/client && next",
    "prebuild": "rimraf dist",
    "build": "NODE_ENV=production nest build && cd src/client && next build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch --preserveWatchOutput",
    "start:debug": "nest start --debug --watch --preserveWatchOutput",
    "start:prod": "NODE_ENV=production node dist/src/server/main",
    "lint": "eslint \"{src,test}/**/*.{ts,tsx}\"",
    "test": "jest --setupFiles dotenv-flow/config --collect-coverage",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:request": "jest --setupFiles dotenv-flow/config --config ./test/jest-request.json",
    "console": "ts-node -r tsconfig-paths/register src/server/console.ts",
    "typeorm": "typeorm-ts-node-commonjs -d ormconfig.ts",
    "client:generate:schema": "zeus src/schema.gql src/client/app/types --typescript --apollo",
    "cy": "cypress"
  },
  "dependencies": {
    "@apollo/client": "^3.11.10",
    "@apollo/gateway": "^2.9.3",
    "@babel/runtime": "^7.26.0",
    "@nestjs/apollo": "^10.2.0",
    "@nestjs/common": "^9.4.3",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^9.4.3",
    "@nestjs/graphql": "^10.2.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^9.4.3",
    "@nestjs/typeorm": "^10.0.2",
    "apollo-server-express": "^3.13.0",
    "axios": "^1.7.8",
    "commander": "^12.1.0",
    "cookie-parser": "^1.4.7",
    "google-auth-library": "^9.15.0",
    "graphql": "^16.9.0",
    "nestjs-console": "^9.0.0",
    "next": "^15.0.3",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "passport-oauth2": "^1.8.0",
    "pg": "^8.13.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.1",
    "ts-morph": "^24.0.0",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.2.0",
    "@nestjs/cli": "^10.4.8",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^10.4.9",
    "@next/eslint-plugin-next": "^15.0.3",
    "@types/cookie-parser": "^1.4.8",
    "@types/express": "^5.0.0",
    "@types/gtag.js": "^0.0.20",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.9.4",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/passport-oauth2": "^1.4.17",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "cypress": "^13.16.0",
    "dotenv-flow": "^4.1.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "fishery": "^2.2.2",
    "graphql-zeus": "^7.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.2"
  }
}
