{
  "name": "nest-typescript-starter",
  "private": true,
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "nest": "nest",
    "next": "cd src/client && next",
    "prebuild": "rimraf dist",
    "build": "NODE_ENV=production nest build && cd src/client && next build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch --preserveWatchOutput",
    "start:debug": "nest start --debug --watch --preserveWatchOutput",
    "start:prod": "NODE_ENV=production node dist/src/server/main",
    "lint": "eslint \"{src,test}/**/*.{ts,tsx}\"",
    "test": "jest --setupFiles dotenv-flow/config --collect-coverage",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:request": "jest --setupFiles dotenv-flow/config --config ./test/jest-request.json",
    "console": "ts-node -r tsconfig-paths/register src/server/console.ts",
    "typeorm": "typeorm-ts-node-commonjs -d ormconfig.ts",
    "client:generate:schema": "zeus src/schema.gql src/client/app/types --typescript --apollo",
    "cy": "cypress"
  },
  "dependencies": {
    "@apollo/client": "^3.7.16",
    "@apollo/gateway": "^2.4.8",
    "@babel/runtime": "^7.22.6",
    "@nestjs/apollo": "^10.2.0",
    "@nestjs/common": "^9.4.3",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^9.4.3",
    "@nestjs/graphql": "^10.2.0",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^9.4.3",
    "@nestjs/typeorm": "^10.0.0",
    "apollo-server-express": "^3.12.0",
    "axios": "^1.4.0",
    "commander": "^11.0.0",
    "cookie-parser": "^1.4.6",
    "google-auth-library": "^8.9.0",
    "graphql": "^16.7.1",
    "nestjs-console": "^8.0.0",
    "next": "^13.4.9",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "passport-oauth2": "^1.7.0",
    "pg": "^8.11.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.1",
    "rxjs": "^7.8.1",
    "ts-morph": "^19.0.0",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@nestjs/cli": "^10.1.7",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^9.4.3",
    "@next/eslint-plugin-next": "^13.4.8",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.17",
    "@types/gtag.js": "^0.0.12",
    "@types/jest": "^28.1.8",
    "@types/node": "^20.4.0",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "@types/passport-oauth2": "^1.4.12",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "cypress": "^12.16.0",
    "dotenv-flow": "^3.2.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "fishery": "^2.2.2",
    "graphql-zeus": "^5.3.0",
    "jest": "^28.1.3",
    "prettier": "^3.0.0",
    "supertest": "^6.3.3",
    "ts-jest": "^28.0.8",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  }
}
